# Google Apps Script Web App URL for RSVP form
NUXT_PUBLIC_GOOGLE_SHEET_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Email SMTP config (used by nuxt-mail)
NUXT_MAIL_SMTP_HOST=smtp.gmail.com
NUXT_MAIL_SMTP_PORT=587
NUXT_MAIL_SMTP_USER=your-email@gmail.com
NUXT_MAIL_SMTP_PASS=your-app-specific-password
NUXT_MAIL_TO=your-email@gmail.com
NUXT_MAIL_FROM=Dan & Jane Wedding <your-email@gmail.com>
NUXT_RSVP_URL=https://dan-jane-wedding.com
